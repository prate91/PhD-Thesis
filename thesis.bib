@inproceedings{potnisGeospatialKnowledgeGraph2023a,
  title = {Towards {{Geospatial Knowledge Graph Infused Neuro-Symbolic AI}} for {{Remote Sensing Scene Understanding}}},
  booktitle = {{{IGARSS}} 2023 - 2023 {{IEEE International Geoscience}} and {{Remote Sensing Symposium}}},
  author = {Potnis, Abhishek and Lunga, Dalton and Sorokine, Alexandre and Dias, Philipe and Yang, Lexie and Arndt, Jacob and Bowman, Jordan and Wohlgemuth, Jason},
  year = {2023},
