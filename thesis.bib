@article{sottiliPalazzoNiccoliniDue2003,
  title = {Palazzo {{Niccolini}}. {{Due}} Episodi Inediti Di 'grandeur' Architettonica Di {{Ferdinando Ruggieri}} e {{Pietro Hostini}} Nella {{Firenze}} Della Prima Met{\`a} Del '700},
  author = {Sottili, Fabio},
  year = {2003},
  journal = {Mitteilungen des Kunsthistorischen Institutes in Florenz},
  volume = {47},
