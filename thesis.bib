@article{hamdaniOntologyBasedFrameworkGeospatial2023,
  title = {An {{Ontology-Based Framework}} for {{Geospatial Integration}} and {{Querying}} of {{Raster Data Cube Using Virtual Knowledge Graphs}}},
  author = {Hamdani, Younes and Xiao, Guohui and Ding, Linfang and Calvanese, Diego},
  year = {2023},
